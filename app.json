{
  "expo": {
    "name": "Localizar",
    "slug": "localizar",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/logoPonto.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashPonto.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-localizarPonto.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.phoenixfoxx.localizar"
    },
    "web": {
      "favicon": "./assets/faviconPonto.png"
    },
    "extra": {
      "eas": {
        "projectId": "84bef1ab-a74f-4e2f-9f71-32b50f92660c"
      }
    },
    "owner": "phoenixfoxx",
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ]
  }
}
